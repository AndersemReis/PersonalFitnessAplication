<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.personafitnessapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.PersonaFitnessApplication">
        <activity
            android:name=".ListaExerciciosActivity"
            android:exported="false" />

        <provider
            android:name=".PersonalProvider"
            android:authorities="com.example.personafitnessapplication"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".EditarAlunoActivity"
            android:exported="false" />
        <activity
            android:name=".InserirAlunoActivity"
            android:exported="false" />
        <activity
            android:name=".ListaAlunosActivity"
            android:exported="false" />
        <activity
            android:name=".PerfilActivity"
            android:exported="true" />
        <activity
            android:name=".FormCadastroActivity"
            android:exported="true" />
        <activity
            android:name=".PrincipalActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>